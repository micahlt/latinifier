  {
    "name": "Latinifier for Wiktionary",
    "version": "1.2",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "description": "Automatically jump to the Latin definition on Wiktionary",
    "permissions": ["storage", "webNavigation", "https://en.wiktionary.org/wiki/*", "tabs", "contextMenus"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://en.wiktionary.org/wiki/*"]
  }],
    "manifest_version": 2
  }
